            {block:ifSideLink3}<li><a href="{text:Side Link 3 URL}"><span class="numbers">03</span>{text:Side Link 3}</a></li>{/block:ifSideLink3}
            {block:ifSideLink4}<li><a href="{text:Side Link 4 URL}"><span class="numbers">04</span>{text:Side Link 4}</a></li>{/block:ifSideLink4}
